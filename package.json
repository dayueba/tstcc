    "ioredis": "^5.3.2",
    "mysql2": "^3.6.5",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
