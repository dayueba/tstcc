  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "start:example": "node dist/example/example.js",
    "start:production": "node dist/example/production-example.js",
    "start:server": "node dist/example/server-example.js",
    "start:mock-services": "node dist/example/mock-service.js",
    "start:client": "node dist/example/client-example.js",
    "dev:example": "ts-node example/example.ts",
    "dev:production": "ts-node example/production-example.ts",
    "dev:server": "ts-node example/server-example.ts",
    "dev:mock-services": "ts-node example/mock-service.ts",
    "dev:client": "ts-node example/client-example.ts",
    "demo": "concurrently \"npm run dev:mock-services\" \"npm run dev:server\"",
    "lint": "tslint -p tsconfig.json -c tslint.json -t stylish \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
